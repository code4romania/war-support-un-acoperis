{
    "name": "code4romania/help-for-health",
    "type": "project",
    "description": "Help For Health.",
    "keywords": [],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "anhskohbo/no-captcha": "^3.3",
        "area17/twill": "2.1.1",
        "aws/aws-sdk-php": "^3.149",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "flynsarmy/csv-seeder": "^2.0",
        "fruitcake/laravel-cors": "^1.0",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^6.3",
        "langleyfoxall/laravel-nist-password-rules": "^4.3",
        "laravel/framework": "^7.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.1",
        "league/flysystem-aws-s3-v3": "^1.0",
        "owen-it/laravel-auditing": "^11.0",
        "pragmarx/google2fa-laravel": "^1.3",
        "propaganistas/laravel-phone": "^4.2",
        "sentry/sentry-laravel": "^2.3",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-permission": "^3.13",
        "spatie/laravel-sluggable": "^2.5",
        "teamtnt/laravel-scout-tntsearch-driver": "^9.0",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
